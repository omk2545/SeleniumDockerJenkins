pipeline {
    agent none
    stages {
        stage('Build Jar') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('Build Image') {
            steps {
                script {
			sh 'echo  building'
                	
                }
            }
        }
        stage('Push Image') {
            steps {
                script {
			
			sh 'echo  building'
			
                }
            }
        }
    }
}
